{
     "name": "nonuser/laravel-fonnte",
     "description": "Unofficial Laravel Fonnte Integration Package",
     "type": "library",
     "license": "MIT",
     "authors": [
          {
               "name": "nonUser",
               "email": "nonuser0002@gmail.com"
          }
     ],
     "require": {
          "php": "^8.2",
          "illuminate/support": "^12.0",
          "illuminate/http": "^12.0",
          "guzzlehttp/guzzle": "^7.2"
     },
     "autoload": {
          "psr-4": {
               "Fonnte\\Laravel\\": "src/"
          }
     },
     "extra": {
          "laravel": {
               "providers": [
                    "Fonnte\\Laravel\\FonnteServiceProvider"
               ],
               "aliases": {
                    "Fonnte": "Fonnte\\Laravel\\Facades\\Fonnte"
               }
          }
     },
     "minimum-stability": "stable",
     "prefer-stable": true
}